<!-- Left 4 Dead Launcher
---- Automate switching between two versions
---- of the game by means file renaming
---- and VPK management -->
<html>
<head>
  <title>Left 4 Dead Launcher</title>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
  <hta:application
    id="steamstyler"
    applicationname="Left 4 Dead Launcher"
    version="1.0"
    border="dialog"
    innerborder="no"
    caption="yes"
    sysmenu="yes"
    maximizebutton="no"
    minimizebutton="yes"
    icon="launcher.ico"
    scroll="no"
    scrollflat="yes"
    singleinstance="yes"
    showintaskbar="yes"
    contextmenu="no"
    selection="no"
  />
  <meta http-equiv="x-ua-compatible" content="ie=9"/>
  <style type="text/css">
body {
  border:0;
  padding: 0;
  margin: 0;
  color: #999;
  background-color: #303036;
  /* IE bug: without this empty SVG line on body
  // the SVG on drop-down style chooser won't show up.
  // It kind of triggers proper SVG initialization
  // or some sort of it? Discovered by accident. */
  background-image: url("data:image/svg+xml;charset=UTF-8,");
  font-family: 'Segoe UI', Verdana, Arial, sans-serif;
}

/* Needed to apply window background gradient */
#content {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  -ms-filter: "progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr=#303036,endColorstr=#28282c)";
}

/* Center all controls vertically */
#center {
  position: absolute;
  left: 0;
  top: 50%;
  width: 100%;
  padding-left: 1em;
  padding-right: 1em;
  transform: translateY(-50%);
  box-sizing: border-box;
}

/* Status */
#status_outer {
  width: 100%;
  display: table;
}

#status {
  color: #666;
  font-size: 85%;
  text-align: center;
  height: 2em;
  display: table-cell;
  width: 50%;
  /*position: relative;
  top: 0em;*/
}

/* Links */
a, a:link, a:visited {
  color: #666;
  text-decoration: underline;
}

a:hover {
  color: #9af;
}

a:active {
  color: #faa0ff;
}

.disabled a {
  color: #666;
  border-color: #666;
}

#status a, #status a:link, #status a:visited {
  color: #666 !important;
}

#status a:hover {
  color: #9af !important;
}

#status a:active {
  color: #faa0ff !important;
}

/* Check box */
#ronly {
  color: #666;
  font-size: 85%;
  text-align: center;
  height: 2em;
  display: table-cell;
  width: 50%;
}

#ronly b {
  border: 2px #78d solid;
  border-radius: 2px;
  width: 1em;
  height: 1em;
  margin-right: 0.5em;
  display: inline-block;
  position: relative;
  top: .25em;
}

#ronly b:before {
  content: '';
  position: absolute;
  top: -.2em;
  left: 0;
  width: 1em;
  height: 1em;
  font-weight: bold;
}

#ronly.on b:before {
  content: '✓';
  color: #78d;
}

#ronly.on:hover b:before {
  content: '✓';
  color: #9af;
}

#ronly.on:active b:before {
  content: '✓';
  color: #faa0ff;
}

#ronly:hover {
  color: #aaa;
}

#ronly:hover b {
  border-color: #9af;
}

#ronly:active b {
  border-color: #faa0ff;
}

#ronly.disabled {
  color: #666 !important;
}

#ronly.disabled b {
  border-color: #666 !important;
}

#ronly.disabled b:before {
  color: #666 !important;
}

/* Buttons */
#buttons {
  text-align: center;
  width: 100%;
  border-collapse:separate;
  display: table;
  border-spacing: 1.5em;
}

#buttons .outer {
  display: table-row;
}

#buttons .button {
  background-repeat: no-repeat;
  background-size: 100% 100%;
  width: 100%;
  height: 15em;
  border-radius: 12px;
  position: relative;
  display: table-cell;
  cursor: pointer;
}

#buttons.disabled .button {
  -ms-filter: "progid:DXImageTransform.Microsoft.BasicImage(grayScale=1)";
}

#buttons.disabled .button:hover .button_overlay {
  -ms-filter: "" !important;
  display: none !important;
}

#buttons .button:hover .button_overlay2 {
  position: absolute;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 15em;
  background: white;
  border-radius: 12px;
  -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=10)";
  z-index: 1;
}

#buttons .button:hover .button_shadow {
  position: absolute;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 15em;
  border: 1px white solid;
  box-sizing: border-box;
  border-radius: 12px;
  -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=10)";
  z-index: 2;
}

#buttons .button:hover .button_overlay {
  position: absolute;
  top: -80px;
  left: -80px;
  width: 100%;
  height: 15em;
  border-radius: 12px;
  -ms-filter: "progid:DXImageTransform.Microsoft.Blur(pixelRadius=80)";
  z-index: -1;
}

.l4d2 {
  background-image: url(data:image/jpeg;base64,);
}

.l4d1 {
  background-image: url(data:image/jpeg;base64,);
}
  </style>
  <script type="text/javascript">
var isDebugging = false;
var isDead = false;
var fso;
var shell;

var makeRoDefault = "no";
var vpksDefault = ["l4d1.vpk"];
var vpksStateDefault = [1];

var gminfo;
var gminfo_l4d2;
var gminfo_l4d1;
var addonList;
var addonsDir;
var mediaDir;
var l4d1Dir;
var l4d2;
var configPath;
var steamPath;
var makeRo;
var vpks;
var vpksState;

var icnError = 16;
var icnWarn = 48;
var icnInfo = 64;
var icnAsk = 32;

/* Dialogs */

var btnOk = 0;
var btnOkCancel = 1;
var btnAbortIgnoreRetry = 2;
var btnYesNoCancel = 3;
var btnYesNo = 4;
var btnRetryCancel = 5;

var retOk = 1;
var retCancel = 2;
var retAbort = 3;
var retRetry = 4;
var retIgnore = 5;
var retYes = 6;
var retNo = 7;

function dialog (type, btns, msg, title) {
  return shell.Popup (msg, 0, title, type | btns);
}

function dlgError (btns, msg, title) {
  return dialog (icnError, btns, msg, title);
}

function dlgWarn (btns, msg, title) {
  return dialog (icnWarn, btns, msg, title);
}

function dlgInfo (btns, msg, title) {
  return dialog (icnInfo, btns, msg, title);
}

function dlgAsk (btns, msg, title) {
  return dialog (icnAsk, btns, msg, title);
}

function msgError (msg, title) {
  return dlgError (btnOK, msg, title);
}

function msgWarn (msg, title) {
  return dlgWarn (btnOk, msg, title);
}

function msgInfo (msg, title) {
  return dlgInfo (btnOk, msg, title);
}

function msgAsk (msg, title) {
  return dlgAsk (btnOkCancel, msg, title);
}

function message (msg) {
  alert (msg);
}

function silence (msg) {}
var debug = isDebugging ? message : silence;

/* File system */

function fileExists (path) {
  return fso.FileExists (path);
}

function dirExists (path) {
  return fso.FolderExists (path);
}

function readFile (path) {
  var forReading = 1;
  var file = fso.OpenTextFile (path, forReading);
  var text = file.ReadAll();
  file.Close();
  return text;
}

function writeFile (path, text) {
  var forWriting = 2;
  var file = fso.OpenTextFile (path, forWriting, true);
  file.Write (text);
  file.Close();
  return fileExists (path);
}

function deleteFile (path) {
  try {
    fso.DeleteFile (path);
  } catch (err) {
    return false;
  }
  return true;
}

function moveFile (from, to) {
  try {
    fso.MoveFile (from, to);
  } catch (err) {
    return false;
  }
  return true;
}

/* String */

function trimLeft (str) {
  var idx = 0;
  if (str.length === 0) return str;
  while (str.charCodeAt (idx) < 32) ++idx;
  return str.substr (idx);
}

function trimRight (str) {
  var len = str.length;
  if (len === 0) return str;
  while (str.charCodeAt (--len) < 32);
  return str.substr (0, len + 1);
}

function trim (str) {
  return trimLeft (trimRight (str));
}

/* HTML DOM */

function $ (id) {
  return document.getElementById (id);
}

function getAttr (e, name) {
  return e.getAttribute (name) || "";
}

function hasClass (e, name) {
  var curr = getAttr (e, "class");
  var idx = curr.indexOf (name);
  return idx != -1 && (idx + name.length == curr.length
  || curr.charCodeAt (idx + name.length) == 32);
}

function addClass (e, name) {
  if (!hasClass (e, name)) e.setAttribute ("class"
  , getAttr (e, "class") + ' ' + name);
}

function removeClass (e, name) {
  e.setAttribute ("class", getAttr (e, "class")
  .replace (new RegExp ("\s?" + name + "\s?", "i"), ""));
}

/* --- */

function disable() {
  if (isDead) return;
  isDead = true;
  addClass ($("buttons"), "disabled");
  addClass ($("status"), "disabled");
  addClass ($("ronly"), "disabled");
}

function enable() {
  removeClass ($("status"), "disabled");
  removeClass ($("buttons"), "disabled");
  removeClass ($("ronly"), "disabled");
  isDead = false;
}

function error (msg) {
  disable();
  msgWarn (msg);
  return false;
}

function errorNotFound (path, dir) {
  disable();
  msgWarn ("Couldn't find "
  + (dir ? "folder" : "file")
  + ': "' + path + '"');
  return false;
}

function placeWindow() {
  var width  = Math.max (600 * /*screen.availWidth*/screen.width / 1920, 600);
  var height = Math.max (width * 1.1, 660);
  window.resizeTo (width, height);
  window.moveTo ((screen.availWidth - width) / 2
  , (screen.availHeight - height) / 2);
  window.focus();
}

/* Configuration */

function readLine (file) {
  var line = "";
  try {
    line = trim (file.ReadLine());
  } catch (e) {}
  return line;
}

function readCfgLine (key, line) {
  var kv = line.split ('=');
  if (kv.length !== 2 || key !== kv[0]) {
    return undefined;
  }
  return kv[1];
}

function readConfig (path) {
  if (!fileExists (path)) {
    makeRo = makeRoDefault;
    vpks = vpksDefault;
    vpksState = vpksStateDefault;

    if (!writeConfig (path)) {
      return false;
    }
  }

  var forReading = 1;
  var file = fso.OpenTextFile (path, forReading);

  makeRo = readCfgLine ("make_addonlist_read_only", readLine (file));
  if (makeRo !== "yes" && makeRo !== "no") {
    makeRo = makeRoDefault;
  }

  var vpks_intro = readCfgLine ("l4d1_addon_vpks", readLine (file));
  if (vpks_intro !== "") {
    vpks = vpksDefault;
    vpksState = vpksStateDefault;
  } else {
    vpks = [];
    vpksState = [];
    while (!file.AtEndOfStream) {
      var line = trim (file.ReadLine());
      var kv = line.split (' ');
      var state = parseInt (kv[1], 10);
      if (!isNaN (state)) {
        vpks.push (kv[0]);
        vpksState.push (state);
      }
    }
  }

  file.Close();
  return true;
}

function writeConfig (path) {
  var forWriting = 2;
  var file = fso.OpenTextFile (path, forWriting, true);
  file.WriteLine ("make_addonlist_read_only=" + makeRo);
  file.WriteLine ("l4d1_addon_vpks=");
  for (var i = 0; i < vpks.length; ++i) {
    file.WriteLine (vpks[i] + " " + vpksState[i]);
  }
  file.Close();
  return fileExists (path);
}

/* Actions */

var cinematics;
var cinematics_l4d2;
var cinematics_l4d1;

function launch (l4d2) {

  function is_whole_word (line, term, idx) {
    return (idx === 0 || line[idx - 1] === '"'
    || line.charCodeAt (idx - 1) < 33)
    && (line[idx + term.length] === '"'
    || (line.charCodeAt (idx + term.length) < 33));
  }

  function find_switch (line, term, idx) {
    idx += term.length;
    while (idx < line.length) {
      if (line[idx] == '0' || line[idx] == '1') {
        return idx;
      }
      ++idx;
    }
    return -1;
  }

  for (var i = 0; i < cinematics.length; ++i) {
    if (!moveFile (cinematics[i], cinematics_l4d2[i])) {
      moveFile (cinematics[i], cinematics_l4d1[i]);
    }
  }

  shell.run ('attrib -R "' + addonList + '"', 0, true);

  var forReading = 1;
  var file = fso.OpenTextFile (addonList, forReading);
  vpk_list = [];
  while (!file.AtEndOfStream) {
    var line = file.ReadLine();
    for (var i = 0; i < vpks.length; ++i) {
      vline = vpks[i];
      var idx = line.indexOf (vline);
      if (idx !== -1 && is_whole_word (line, vline, idx)) {
        idx = find_switch (line, vline, idx);
        if (idx !== -1) {
          line = line.substring (0, idx)
          + (l4d2 ? 1 - vpksState[i] : vpksState[i])
          + line.substring (idx + 1);
          break;
        }
      }
    }
    vpk_list.push (line);
  }
  file.Close();

  var forWriting = 2;
  file = fso.OpenTextFile (addonList + ".new", forWriting, true);
  for (var i = 0; i < vpk_list.length; ++i) {
    file.WriteLine (vpk_list[i]);
  }
  file.Close();

  deleteFile (addonList);
  moveFile (addonList + ".new", addonList);

  if (makeRo === "yes") {
    shell.run ('attrib +R "' + addonList + '"', 0, true);
  }

  writeConfig (configPath);
}

function launchL4D2() {
  if (isDead) return;
  launch(true);
  for (var i = 0; i < cinematics.length; ++i) {
    moveFile (cinematics_l4d2[i], cinematics[i]);
  }
  fso.CopyFile (gminfo_l4d2, gminfo, true);
  shell.Run ('"' + steamPath + '" -applaunch 550', 1, false);
  window.close();
}

function launchL4D1() {
  if (isDead) return;
  launch(false);
  for (var i = 0; i < cinematics.length; ++i) {
    moveFile (cinematics_l4d1[i], cinematics[i]);
  }
  fso.CopyFile (gminfo_l4d1, gminfo, true);
  shell.Run ('"' + steamPath + '" -applaunch 550', 1, false);
  window.close();
}

function toggleReadOnly() {
  if (isDead) return;
  var ronly = $("ronly");
  if (hasClass (ronly, "on")) {
    removeClass (ronly, "on");
    makeRo = "no";
  } else {
    addClass (ronly, "on");
    makeRo = "yes";
  }
}

function getSteamPath() {
  path = "";
  try {
    path = shell.RegRead ("HKLM\\SOFTWARE\\Valve\\Steam\\InstallPath");
  } catch (e) {
    try {
      path = shell.RegRead ("HKLM\\SOFTWARE\\WOW6432Node\\Valve\\Steam\\InstallPath");
    } catch (e) {
      return "";
    }
  }
  return fso.BuildPath (path, "steam.exe");
}

function buyCoffee() {
  shell.Run ("https://www.buymeacoffee.com/ubihazard", 1, false);
}

/* === */

function main() {
  /* Position window on screen */
  placeWindow();

  /* Initialize */
  shell = new ActiveXObject ("WScript.Shell");
  fso = new ActiveXObject ("Scripting.FileSystemObject");
  var currDir = fso.GetAbsolutePathName (".");
  configPath = fso.BuildPath (currDir, "launcher.cfg");
  gminfo = fso.BuildPath (currDir, "left4dead2\\gameinfo.txt");
  gminfo_l4d2 = fso.BuildPath (currDir, "left4dead2\\gameinfo.l4d2.txt");
  gminfo_l4d1 = fso.BuildPath (currDir, "left4dead2\\gameinfo.l4d1.txt");
  addonList = fso.BuildPath (currDir, "left4dead2\\addonlist.txt");
  addonsDir = fso.BuildPath (currDir, "left4dead2\\addons");
  mediaDir = fso.BuildPath (currDir, "left4dead2\\media");
  l4d1Dir = fso.BuildPath (currDir, "left4dead1");
  l4d2 = fso.BuildPath (currDir, "left4dead2.exe");

  cinematics = [
    fso.BuildPath (mediaDir, "l4d2_background01.bik"),
    fso.BuildPath (mediaDir, "l4d2_background02.bik"),
    fso.BuildPath (mediaDir, "l4d2_background03.bik"),
    fso.BuildPath (mediaDir, "l4d2_background04.bik"),
    fso.BuildPath (mediaDir, "l4d2_background05.bik")
  ];

  cinematics_l4d2 = [
    fso.BuildPath (mediaDir, "l4d2_background01.l4d2.bik"),
    fso.BuildPath (mediaDir, "l4d2_background02.l4d2.bik"),
    fso.BuildPath (mediaDir, "l4d2_background03.l4d2.bik"),
    fso.BuildPath (mediaDir, "l4d2_background04.l4d2.bik"),
    fso.BuildPath (mediaDir, "l4d2_background05.l4d2.bik")
  ];

  cinematics_l4d1 = [
    fso.BuildPath (mediaDir, "l4d2_background01.l4d1.bik"),
    fso.BuildPath (mediaDir, "l4d2_background02.l4d1.bik"),
    fso.BuildPath (mediaDir, "l4d2_background03.l4d1.bik"),
    fso.BuildPath (mediaDir, "l4d2_background04.l4d1.bik"),
    fso.BuildPath (mediaDir, "l4d2_background05.l4d1.bik")
  ];

  /* Read configuration */
  if (!readConfig (configPath)) {
    return error ("Couldn't read the configuration file.");
  }

  /* Check for the presence of important files & folders */
  var path;
  var ok = true;
  if (!fileExists (path = gminfo)) return errorNotFound (path);
  if (!fileExists (path = gminfo_l4d2)) return errorNotFound (path);
  if (!fileExists (path = gminfo_l4d1)) return errorNotFound (path);
  if (!fileExists (path = addonList)) return errorNotFound (path);
  if (!fileExists (path = l4d2)) return errorNotFound (path);
  if (!dirExists (path = addonsDir)) return errorNotFound (path, true);
  if (!dirExists (path = mediaDir)) return errorNotFound (path, true);
  if (!dirExists (path = l4d1Dir)) return errorNotFound (path, true);

  /* Get Steam path */
  steamPath = getSteamPath();
  if (steamPath === "") {
    disable();
    return error ("Steam doesn't appear to be installed.");
  }
  if (!fileExists (steamPath)) {
    disable();
    return error ("Couldn't find Steam.");
  }

  if (makeRo === "yes") {
    addClass ($("ronly"), "on");
  }

  return true;
}
  </script>
</head>
<body onload="main()" ondragstart="return false;" ondrop="return false;">
<div id="content"><div id="center">
  <div id="buttons">
    <div class="outer"><div class="button l4d2" onclick="launchL4D2()"><div class="button_overlay l4d2"></div><div class="button_overlay2"></div><div class="button_shadow"></div></div></div>
    <div class="outer"><div class="button l4d1" onclick="launchL4D1()"><div class="button_overlay l4d1"></div><div class="button_overlay2"></div><div class="button_shadow"></div></div></div>
  </div>
  <div id="status_outer">
    <div id="ronly" onclick="toggleReadOnly()"><b></b>Make addonlist.txt read-only</div>
    <div id="status">Version 1.0 by <a id="coffee" title="Buy me a coffee!" href="javascript:void()" onclick="buyCoffee()">Ubihazard</a></div>
  </div>
</div></div>
</body>
</html>
